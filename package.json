	"dependencies": {
		"bootstrap-scss": "^4.4.1",
		"bootstrap-vue": "^2.11.0",
		"normalize-scss": "^7.0.1",
		"vue": "^2.6.11",
		"vuex": "^3.1.3"
	}
}
